        },
        "font-awesome": {
            "main": [
                "./css/font-awesome.css",
                "./fonts/*"
            ]
        },
        "jQuery.mmenu": {
            "main": [
                "./dist/jquery.mmenu.all.css",
                "./dist/jquery.mmenu.all.js"
            ]
        },
        "slick-carousel": {
            "main": [
                "./slick/slick.css",
                "./slick/slick-theme.css",
                "./slick/slick.js",
                "./fonts/*"
            ]
        },
        "jquery-ui":{
            "main": [
                "./themes/vader/jquery-ui.min.css",
                "./jquery-ui.min.js"
            ]
        }
    }
